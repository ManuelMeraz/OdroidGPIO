# - Config file for the odroid-gpio package
# It defines the following variables
#  ODROID_GPIO_INCLUDE_DIRS - include directories for odroid-gpio
#  ODROID_GPIO_LIBRARIES    - libraries to link against
#  ODROID_GPIO_EXECUTABLE   - the bar executable

# Compute paths
get_filename_component(ODROID_GPIO_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(ODROID_GPIO_INCLUDE_DIRS "@CONF_INCLUDE_DIRS@")

# Our library dependencies (contains definitions for IMPORTED targets)
include("${ODROID_GPIO_CMAKE_DIR}/odroid-gpio-targets.cmake")

# These are IMPORTED targets created by odroid-gpio-targets.cmake
set(ODROID_GPIO_LIBRARIES odroid-gpio)
